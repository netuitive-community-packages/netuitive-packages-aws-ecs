{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "^aws\\.ecs\\.(cpu.*|memory.*)",
        "properties": {
          "tags": {
            "unit": "percent"
          },
          "validMax": 100
        }
      },
      {
        "match": "^aws\\.ecs\\.(cpu|memory)utilization",
        "properties": {
          "tags": {
            "utilization": true
          }
        }
      }
    ],
    "name": "AWS ECS Service",
    "scope": {
      "elementType": "ECS Service"
    },
    "type": "METRIC_META"
  }
}